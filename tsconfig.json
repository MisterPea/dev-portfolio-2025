{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}